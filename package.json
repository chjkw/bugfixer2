{
	"name": "bugfixer",
	"private": true,
	"displayName": "Bug Fixer",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:c"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "bugfixer.analyze_all",
				"title": "Bugfixer 실행: 처음부터"
			},
			{
				"command": "bugfixer.analyze_continue",
				"title": "Bugfixer 실행: 이어서"
			}
		],
		"configuration": [
			{
				"type": "object",
				"title": "Bugfixer 설정",
				"properties": {
					"bugfixer.log_level": {
						"type": "string",
						"default": "trace",
						"enum": [
							"info",
							"debug",
							"trace"
						],
						"description": "Log Level"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"eslint": "^8.13.0",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"install": "^0.13.0",
		"npm": "^8.15.0",
		"winston": "^3.8.1"
	}
}
